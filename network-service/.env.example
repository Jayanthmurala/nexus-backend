# Network Service Environment
PORT=4005
# Important: include schema=networksvc to target this service's schema
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nexus?schema=networksvc

# Auth (auth-service)
AUTH_JWKS_URL=http://localhost:4001/.well-known/jwks.json
AUTH_JWT_ISSUER=nexus-auth
AUTH_JWT_AUDIENCE=nexus

# Ads / Internal signing
ADVANCEMENT_HMAC_SECRET=change-me-advancement
