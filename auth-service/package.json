    "fastify": "^4.29.1",
    "fastify-type-provider-zod": "^1.2.0",
    "jose": "^5.3.0",
    "mjml": "^4.14.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "pino": "^9.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
