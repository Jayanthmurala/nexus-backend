# Profile Service
NODE_ENV=development
PORT=4002
# Neon Postgres direct connection (use a service-isolated schema)
# Replace the credentials/host/db and keep schema=profilesvc or pick your own schema name
DATABASE_URL="postgresql://USER:PASSWORD@HOST/DB?sslmode=require&schema=profilesvc"

# JWT verification
AUTH_JWKS_URL=http://auth-service:4001/.well-known/jwks.json
AUTH_JWT_ISSUER=nexus-auth
AUTH_JWT_AUDIENCE=nexus
